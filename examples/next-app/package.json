{
  "name": "schema-sentry-example-next-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "schema:validate": "schemasentry validate --manifest ./schema-sentry.manifest.json --data ./schema-sentry.data.json",
    "schema:collect": "schemasentry collect --root ./.next/server/app --output ./schema-sentry.data.collected.json",
    "schema:drift": "schemasentry collect --root ./.next/server/app --check --data ./schema-sentry.data.json",
    "schema:e2e": "node ./scripts/schema-e2e.mjs"
  },
  "dependencies": {
    "@schemasentry/next": "workspace:*",
    "next": "^16.1.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@schemasentry/cli": "workspace:*",
    "typescript": "^5.5.0"
  }
}
