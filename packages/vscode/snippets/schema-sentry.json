  "Schema - Organization": {
    "prefix": "schema-organization",
    "body": [
      "import { Schema, Organization } from '@schemasentry/next';",
      "",
      "const org = Organization({",
      "  name: '$1',",
      "  url: '$2',",
      "  logo: '$3',",
      "});",
      "",
      "export default function OrganizationSchema() {",
      "  return <Schema data={org} />;",
      "}"
    ],
    "description": "Add Organization schema"
  },
